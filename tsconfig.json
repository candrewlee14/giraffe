{
  "compilerOptions": {
    "target": "es2017",
    "module": "esnext",
    "lib": ["es5", "es2015", "es2016", "es2017", "es2018", "esnext", "dom"],
    "sourceMap": true,
    "allowJs": false,
    "jsx": "react",
    "strict": true,
    "declaration": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "noImplicitAny": false,
    "noImplicitThis": false,
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "strictNullChecks": false,
    "downlevelIteration": true,
    "skipLibCheck": true
  }
}
